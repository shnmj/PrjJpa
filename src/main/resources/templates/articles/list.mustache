{{>layouts/header}}

<!-- BootStrap Table 참고 -->

<table class="table">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Title</th>
      <th scope="col">Content</th>
    </tr>
  </thead>
  <tbody>
	  <!-- c:forEach 대신 list 들어오면 알아서 반복 -->
	  
  	{{#articleList}}
    <tr>
      <th scope="row">{{id}}</th>
      <td><a href="/articles/{{id}}">{{title}}</a></td>
      <td>{{content}}</td>
    </tr>
    {{/articleList}}
  </tbody>
</table> 

	<a href="/articles/WriteForm" class="btn btn-primary">새 글쓰기</a>


{{>layouts/footer}}